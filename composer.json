{
    "name": "migo/unit-conversions",
    "description": "unit conversions in php",
    "keywords": [
        "migo",
        "unit-conversions"
    ],
    "homepage": "https://github.com/migo1/unit-conversions",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Migot",
            "email": "ertmigot@gmail.com",
            "homepage": "https://github.com/migo1",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Migo\\UnitConversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migo\\UnitConversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/spatie"
        },
        {
            "type": "other",
            "url": "https://spatie.be/open-source/support-us"
        }
    ]
}
